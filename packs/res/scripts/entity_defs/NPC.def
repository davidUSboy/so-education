<root>

	<Implements>
		<Interface> ItemHolder </Interface>
		<Interface>AvatarInterface </Interface>
		<Interface>Bot </Interface>
		<Interface>BaseAndCell </Interface>
		<Interface>Trader </Interface> 
		<Interface>BaseTrader </Interface> 
		<Interface>MappedObject </Interface> 
		<Interface>InteractiveObject </Interface>
		<Interface> Characteristics </Interface>
		
		<!-- for Brain -->		
		<Interface> Brain_TargeterInterface </Interface>		
		<Interface> Brain_ShooterInterface </Interface>		
		<Interface> Brain_FollowerInterface </Interface>
		<Interface> Brain_PromenadeInterface </Interface>		
		<Interface> Brain_WaypointInterface </Interface>		
