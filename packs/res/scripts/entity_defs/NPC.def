	<Volatile>
		<position/>
		<yaw/>
		<pitch/>
	</Volatile> 

	<Properties>
		<donatBaseName>
			<Type> STRING </Type>
			<Flags> BASE </Flags>
			<Editable>		true			</Editable>
		</donatBaseName>
		
		<owner>
			<Type> MAILBOX </Type>
			<Flags> CELL_PRIVATE </Flags>
			<Persistent>	false			</Persistent>
			<Editable>		false			</Editable>
		</owner>
		<ownerSpotId>
			<Type> INT32 </Type>
			<Flags> CELL_PRIVATE </Flags>
			<Persistent>	false			</Persistent>
			<Editable>		false			</Editable>
		</ownerSpotId>
		<timeBeforeResurrect>
			<Type> FLOAT </Type>
			<Flags> CELL_PRIVATE </Flags>
			<Editable>		true			</Editable>
			<Default> 5.0 </Default>
		</timeBeforeResurrect>
		<impactChance>
			<Type> FLOAT </Type>
			<Flags> CELL_PRIVATE </Flags>
			<Editable>		true			</Editable>
