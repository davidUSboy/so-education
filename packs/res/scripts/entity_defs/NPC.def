			<Type> STRING </Type>
			<Flags> ALL_CLIENTS </Flags>
			<Editable>		true			</Editable>
		</npcName>
		<actionOnLaunch>
			<Type> STRING </Type>
			<Flags> ALL_CLIENTS </Flags>
			<Editable>		true			</Editable>
		</actionOnLaunch>
		
		<ignorePK>
			<Type> BOOL </Type>
			<Flags> CELL_PUBLIC </Flags>
			<Editable>		true			</Editable>
		</ignorePK>
		<displayName>
			<Type> STRING </Type>
			<Flags> CELL_PRIVATE </Flags>
		</displayName>
		<_ServiceTag>
			<Type> STRING </Type>
			<Flags> CELL_PRIVATE </Flags>
