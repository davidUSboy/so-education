<root>	<Implements>		<Interface> TimerManagerBase </Interface>	</Implements>		<Properties>		<!-- Properties for worldeditor http://dwiki.stalker-online.co/index.php/Area_cron -->		<TT_timesCount>			<Type>			UINT16 		</Type>			<Flags>			BASE		</Flags>			<Default>		0 			</Default>			<Editable>		true		</Editable>		</TT_timesCount>				<TT_timeShift>			<Type>			STRING		</Type> <!-- 'HH:MM' format -->			<Flags>			BASE		</Flags>			<Default>		00:00 		</Default>			<Editable>		true		</Editable>		</TT_timeShift>						<TT_filterFrom>			<Type>			STRING		</Type> <!-- 'HH:MM' format -->			<Flags>			BASE		</Flags>			<Default>		00:00 		</Default>			<Editable>		true		</Editable>		</TT_filterFrom>							<TT_filterTo>			<Type>			STRING		</Type> <!-- 'HH:MM' format -->			<Flags>			BASE		</Flags>			<Default>		00:00 		</Default>			<Editable>		true		</Editable>		</TT_filterTo>					<!-- Other data -->		<TT_timesList>			<Type>			ARRAY <of> INT16 </of> 	</Type>			<Flags>			BASE					</Flags>			<Persistent>	true					</Persistent>		</TT_timesList>				<TT_lastTime>			<Type>			INT16 		</Type>			<Flags>			BASE		</Flags>			<Default>		-1			</Default> <!-- no last time at start -->					</TT_lastTime>			</Properties>	<CellMethods>	</CellMethods>	<BaseMethods>		<TT_onTimeTrigger>			<Arg> INT16 </Arg> <!-- next trigger time  -->		</TT_onTimeTrigger>		</BaseMethods></root>