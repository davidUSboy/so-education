<root>
	<BaseMethods>
		<SendMail>
			<Arg> INT32 </Arg><!-- Message ID generated by sender's client -->
			<Arg> INT32 </Arg><!-- Mail Kind -->
			<Arg> UNICODE_STRING </Arg><!-- Text -->
			<Arg> INT32	</Arg><!-- To Kind -->
			<Arg> ARRAY <of> INT32 </of> </Arg><!-- To ID -->
			<Arg> ARRAY <of> UNICODE_STRING</of> </Arg><!-- To Name - if ToID==0 -->
			<Arg> UNICODE_STRING </Arg><!-- From Name -->
		</SendMail>
		<PythonLog>
			<Arg> UNICODE_STRING </Arg><!-- Text -->
		</PythonLog>
		<!-- New chat -->
		<targetMessage>
			<Exposed/>
			<Arg> UNICODE_STRING </Arg>
		</targetMessage>
		<runCommand>
			<Arg> UNICODE_STRING </Arg> <!-- Raw command text from client -->			
		</runCommand>
	</BaseMethods>
	
	<CellMethods>
		<SendMail>
			<Exposed/>
			<Arg> INT32 </Arg><!-- Message ID generated by sender's client -->
			<Arg> INT32 </Arg><!-- Mail Kind -->
			<Arg> UNICODE_STRING </Arg><!-- Text -->
			<Arg> INT32 </Arg><!-- To Kind -->
			<Arg> INT32 </Arg><!-- To ID -->
			<Arg> UNICODE_STRING </Arg><!-- To Name - if ID==0 -->
		</SendMail>
		<ReceiveMail>
			<Arg> INT32 </Arg><!-- Message ID generated by sender's client -->
			<Arg> INT32 </Arg><!-- Mail Kind -->
			<Arg> UNICODE_STRING </Arg><!-- Text -->
			<Arg> INT32 </Arg><!-- From Kind -->
			<Arg> INT32 </Arg><!-- From ID -->
			<Arg> UNICODE_STRING </Arg><!-- From Name -->
		</ReceiveMail>
		<PythonLog>
			<Arg> UNICODE_STRING </Arg><!-- Text -->
		</PythonLog>
		<worldMessage>
			<Exposed/>
			<Arg> UNICODE_STRING </Arg> <!-- Raw message text from client -->
		</worldMessage>
		<runCommand>
			<Exposed/>
			<Arg> UNICODE_STRING </Arg> <!-- Raw command text from client -->			
		</runCommand>
		<putMessage>
			<Arg> UNICODE_STRING </Arg> <!-- Text of the message -->
			<Arg> INT32 </Arg> <!-- Message type -->
			<Arg> UNICODE_STRING </Arg> <!-- Name of the source -->
		</putMessage>
		
	</CellMethods>
	
	<ClientMethods>
		<ReceiveMail>
			<Arg> INT32 </Arg><!-- Message ID generated by sender's client -->
			<Arg> INT32 </Arg><!-- Mail Kind -->
			<Arg> UNICODE_STRING </Arg><!-- Text -->
			<Arg> INT32 </Arg><!-- From Kind -->
			<Arg> INT32 </Arg><!-- From ID -->
			<Arg> UNICODE_STRING </Arg><!-- From Name -->
		</ReceiveMail>
		<PythonLog>
			<Arg> UNICODE_STRING </Arg><!-- Text -->
		</PythonLog>
		
		<systemChatline>
			<Arg> UNICODE_STRING </Arg><!-- Text -->
		</systemChatline>
		<questChatline>
			<Arg> UNICODE_STRING </Arg><!-- Text -->
		</questChatline>
		<guildChatline>
			<Arg> UNICODE_STRING </Arg><!-- Text -->
		</guildChatline>
		<lootChatline>
			<Arg> UNICODE_STRING </Arg><!-- Text -->
		</lootChatline>
		<putMessage>
			<Arg> UNICODE_STRING </Arg> <!-- sent message -->
			<Arg> INT32 </Arg> <!-- Message type -->
			<Arg> UNICODE_STRING </Arg> <!-- source name -->
			<Arg> INT8 </Arg> <!-- is message a delayed one ? interpret as boolean -->
		</putMessage>
		
	</ClientMethods>
	<Properties>
		<lastGlobalMessagesPoll>
			<Type>	INT32		</Type>
			<Flags>	BASE		</Flags>
			<Default> 0 </Default>
			<Persistent>	false	</Persistent>
		</lastGlobalMessagesPoll>
	</Properties>
</root>
