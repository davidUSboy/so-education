<root>

	<Implements>
		<Interface> Victim </Interface>
		<Interface> Damager </Interface>
		<Interface> AnomalyVisitor </Interface>
		<Interface> AreaVisitor </Interface>
		<Interface> Effects</Interface><!-- test -->
	</Implements>

	<LoDLevels>
		<level> 20     <hyst>  4 </hyst> <label> SHORT     </label>     </level>
		<level> 100     <hyst> 10 </hyst> <label> LONG</label>     </level>
		<level> 250     <hyst> 20 </hyst> <label> UNREAL     </label>     </level>
	</LoDLevels>
	
	<Properties>

		<crouching>
			<Type> INT32 </Type>
			<Flags> ALL_CLIENTS </Flags>
			<Default> 0 </Default>
		</crouching>
		<sitting>
			<Type> INT32 </Type>
			<Flags> ALL_CLIENTS </Flags>
			<Default> 0 </Default>
		</sitting>
		<name>
			<Type> UNICODE_STRING </Type>
			<Flags> ALL_CLIENTS </Flags>
			<Persistent>	true	</Persistent>
			<Editable> true </Editable>
			<Identifier>	true	</Identifier> 
		</name>
		<frags>
			<Type> INT32 </Type>
			<Flags> ALL_CLIENTS </Flags>
			<Persistent>	true	</Persistent>
			<Default>0</Default>
		</frags>
		<deaths>
			<Type> INT32 </Type>
			<Flags> ALL_CLIENTS </Flags>
			<Persistent>	true	</Persistent>
			<Default>0</Default>
		</deaths>
				
		<sniping>
			<Type>	BOOL		</Type>
			<Flags>	OTHER_CLIENTS	</Flags>
			<Persistent>	false	</Persistent>
			<Default> 0 </Default>
			<DetailLevel>UNREAL</DetailLevel>
		</sniping>

		<canExplodeGrenade>
			<Type>	BOOL		</Type>
			<Flags>	CELL_PUBLIC		</Flags>
			<Default> 0 </Default>
		</canExplodeGrenade>
		<alreadyExplodedGrenade>
			<Type>	BOOL		</Type>
			<Flags>	CELL_PUBLIC		</Flags>
			<Default> 0 </Default>
		</alreadyExplodedGrenade>
		<grenadePosition>
			<Type>	VECTOR3		</Type>
			<Flags>	CELL_PUBLIC	</Flags>
		</grenadePosition>

		<relationsWithFractions>
			<Type>	ARRAY <of> TUPLE <of>INT32</of></of></Type>
			<Flags>	CELL_PUBLIC	</Flags>
			<Persistent>	true	</Persistent>
		</relationsWithFractions>
		
	</Properties>

	<ClientMethods>

		<SetAvatarModel>
		</SetAvatarModel>
		<onJump>
		</onJump>
		<reload>	
			<Arg> INT8 </Arg>
			<Arg> FLOAT </Arg>
			<Arg> INT32 </Arg>
		</reload>
		
		<fire>
		</fire>

		<DrawThrow>
			<Arg> FLOAT </Arg>
			<Arg> VECTOR3 </Arg><!-- velocity -->
        </DrawThrow>

		<updateGrenadePath>
			<Arg> VECTOR3 </Arg><!-- serverPos -->
			<Arg> VECTOR3 </Arg><!-- destPos -->
		</updateGrenadePath>

		<dropItem>
			<Arg> INT32 </Arg>
		</dropItem>
		
		<DrawUseItem>
			<Arg> INT32 </Arg>
			<Arg> FLOAT </Arg>
		</DrawUseItem>
		<DrawUseItemOnTarget>
			<Arg> INT32 </Arg>
			<Arg> FLOAT </Arg>
		</DrawUseItemOnTarget>
		
		
