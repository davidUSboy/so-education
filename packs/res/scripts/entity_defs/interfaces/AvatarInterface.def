		</putOffItem>

		<reload>
			<Exposed/>
		</reload>
		
		<onJump>
			<Exposed/>
		</onJump>
		
		<fire>
			<Arg>
				ARRAY <of> VICTIM_HIT</of> 
			</Arg>
		</fire>
		
		<throwGrenade>
			<Exposed/>
			<Arg> VECTOR3 </Arg><!-- pos -->
			<Arg> VECTOR3 </Arg><!-- velocity -->
			<Arg> ENTITY_ID </Arg> <!-- client entity id -->
		</throwGrenade>
		
		<throwStone>
			<Exposed/>
			<Arg> VECTOR3 </Arg><!-- pos -->
			<Arg> VECTOR3 </Arg><!-- velocity -->
			<Arg> ENTITY_ID </Arg> <!-- client entity id -->
		</throwStone>	

