		<MaxAppearedItems>
			<Type> INT32 </Type>
			<Flags> CELL_PRIVATE </Flags>
			<Default> 10 </Default>
			<Editable> true </Editable>
		</MaxAppearedItems>
		
		
		<MassUpdate>
			<Type> INT8 </Type>
			<Flags> CELL_PRIVATE </Flags>
			<Default> 0 </Default>
			<Editable> true </Editable>
		</MassUpdate>
		
		<DoRenew>
			<Type> INT8 </Type>
			<Flags> CELL_PRIVATE </Flags>
			<Default> 0 </Default>
			<Editable> true </Editable>
		</DoRenew>
		
		
		<RenewTime>
			<Type> INT32 </Type>
			<Flags> CELL_PRIVATE </Flags>
			<Persistent> true </Persistent>
			<Default> 600 </Default>
			<Editable> true </Editable>
		</RenewTime>
		
		<loots>
			<Type> PYTHON </Type>
			<Flags> CELL_PRIVATE </Flags>
			<Persistent> false </Persistent>
		</loots>
		
