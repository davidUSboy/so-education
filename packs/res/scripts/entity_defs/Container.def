			<Type> UNICODE_STRING  </Type>
			<Flags> ALL_CLIENTS </Flags>
			<Default> </Default>
			<Editable>		true			</Editable>
		</containerName>

		<modelSelect>
			<Type>			INT32			</Type>
			<Flags>			ALL_CLIENTS			</Flags>
		</modelSelect>

		<CarryingItems>
			<Type> ARRAY <of> COMPLEX_ITEM  </of> </Type>
			<Flags> ALL_CLIENTS </Flags>
			<Default> </Default>
		</CarryingItems>
	
		<AppearedItems>
			<Type> ARRAY <of> INT32 </of> </Type>
			<Flags> CELL_PRIVATE </Flags>
			<Default> </Default>
		</AppearedItems>	
		
		<dictCounts>
			<Type> ARRAY <of> FIXED_DICT
						<Properties>
							<itemType>
								<Type> INT32</Type> 
							</itemType>
							<itemCount>
								<Type> INT32</Type> 
							</itemCount>
						</Properties>	
			</of> </Type>
			
			<Flags> CELL_PRIVATE </Flags>
		</dictCounts>
		
		<ContainerListName>
			<Type> ARRAY <of> STRING </of> </Type>
			<Flags>			CELL_PRIVATE		</Flags>
			<Editable>		true			</Editable>
			<Default> </Default>
