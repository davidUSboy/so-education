	<ClientMethods>
	</ClientMethods>

	<CellMethods>
		<GetItemFromHolderClient>
			<Exposed/>
			<Arg> INT32 </Arg>
			<Arg> INT32 </Arg>
		</GetItemFromHolderClient>
	</CellMethods>

  <BaseMethods>
  </BaseMethods>
