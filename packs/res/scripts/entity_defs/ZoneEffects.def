<root>
	<Implements>
		<Interface> BaseAndCell </Interface>
		<Interface> MappedObject </Interface>
		<Interface> EffectsSource </Interface>
		<Interface> TimerManagerCell </Interface>
	</Implements>

	<Properties>
		<name>
			<Type>			STRING			</Type>
			<Flags>			CELL_PRIVATE			</Flags>
			<Persistent>	true			</Persistent>
			<Editable>		true			</Editable>
		</name>

		<radius> 			<!--  TEMP -->
			<Type>			FLOAT 			</Type>
			<Flags>			CELL_PRIVATE			</Flags>
			<Default>		50.000000 			</Default>
			<Persistent>	true			</Persistent>
			<Editable>		true			</Editable>
			<Widget>		RADIUS						<!--  Draw sphere in world editor  --->
				<colour>	255.000000 0.000000 0.000000 255.000000	</colour>
				<gizmoRadius>	2				</gizmoRadius>
			</Widget>
		</radius>

		<volumes>
			<Type>			ARRAY <of> UDO_REF </of> </Type>
			<Flags>			CELL_PRIVATE		</Flags>
			<Persistent>	true				</Persistent>
			<Editable>		true				</Editable>
		</volumes>

		<trapped>
			<Type>			ARRAY <of>MAILBOX</of>	</Type>
			<Flags>			CELL_PUBLIC	    </Flags>
			<Persistent>	false			</Persistent>
			<Editable>		false			</Editable>
		</trapped>

        <listApplyOnEnter>
            <Type>			ARRAY <of>BASE_EFFECT_ID</of>	</Type>
			<Flags>			CELL_PRIVATE	</Flags>
			<Editable>		true			</Editable>
        </listApplyOnEnter>

		<UDOExceptionsCount>
			<Type>			UINT16			</Type>
			<Flags>			CELL_PRIVATE	</Flags>
			<Default>		0 				</Default>
		</UDOExceptionsCount>		
    </Properties>

	<ClientMethods>
	</ClientMethods>

	<CellMethods>

		<onEnterArea>
			<Arg>		MAILBOX	</Arg>
		</onEnterArea>

		<onLeaveArea>
			<Arg>		MAILBOX	</Arg>
		</onLeaveArea>

        <onLeaveAreaID>
            <Arg>		ENTITY_ID	</Arg>
        </onLeaveAreaID>


	</CellMethods>

	<BaseMethods>

	</BaseMethods>

</root>

