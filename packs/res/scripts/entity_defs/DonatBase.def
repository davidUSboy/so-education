<root>
	<Implements>
                <Interface> BaseAndCell </Interface>
				<Interface> TimerManagerCell </Interface>
        		<!--<Interface>Manipulator</Interface>-->
	</Implements>

    <Properties>

		<name>
			<Type>			STRING			</Type>
			<Flags>			CELL_PRIVATE	        </Flags>
			<Editable>      True                     </Editable>
		</name>
		
		<nameOnBase>
			<Type>			STRING			</Type>
			<Flags>			BASE	        </Flags>
			<Editable>      False           </Editable>
		</nameOnBase>
		
		<BaseType>
			<Type>			BASE_TYPE			</Type>
			<Flags>			BASE	        </Flags>
			<Editable>      True                     </Editable>
		</BaseType>
		
		<spaceName>
			<Type>			STRING			</Type>
			<Flags>			CELL_PRIVATE	        </Flags>
			<Editable>      False                     </Editable>
		</spaceName>
		
		<minPrice>
			<Type>			INT32			</Type>
			<Flags>			CELL_PRIVATE	        </Flags>
			<Editable>      False                     </Editable>
		</minPrice>

		<location>
			<Type>			STRING			</Type>
			<Flags>			CELL_PRIVATE	        </Flags>
			<Editable>      False                     </Editable>
		</location>
		
		<guardsUDO>
			<Type>			ARRAY <of> UDO_REF </of> </Type>
			<Flags>			CELL_PRIVATE		</Flags>
            <Editable>      True                     </Editable>
		</guardsUDO>
        <flagsUDO>
			<Type>			ARRAY <of> UDO_REF </of> </Type>
			<Flags>			CELL_PRIVATE		</Flags>
            <Editable>      True                     </Editable>
		</flagsUDO>
        <tradersUDO>
			<Type>			ARRAY <of> UDO_REF </of> </Type>
			<Flags>			CELL_PRIVATE		</Flags>
            <Editable>      True                     </Editable>
		</tradersUDO>
        <volumesUDO>
			<Type>			ARRAY <of> UDO_REF </of> </Type>
			<Flags>			CELL_PRIVATE		</Flags>
            <Editable>      True                     </Editable>
		</volumesUDO>
        <managersUDO>
			<Type>			ARRAY <of> UDO_REF </of> </Type>
			<Flags>			CELL_PRIVATE		</Flags>
            <Editable>      True                     </Editable>
		</managersUDO>
		<UDOExceptionsCount>
			<Type>			UINT16			</Type>
			<Flags>			CELL_PRIVATE	</Flags>
			<Default>		0 				</Default>
		</UDOExceptionsCount>
        <!--<safeUDO>-->
			<!--<Type>			ARRAY <of> UDO_REF </of> </Type>-->
			<!--<Flags>			CELL_PRIVATE		</Flags>-->
            <!--<Editable>      True                     </Editable>-->
		<!--</safeUDO>-->
        <safeAndRaspUDO>
			<Type>			UDO_REF             </Type>
			<Flags>			CELL_PRIVATE		</Flags>
            <Editable>      True                </Editable>
		</safeAndRaspUDO>

        <clanID>
			<Type>			INT64			</Type>
			<Flags>			CELL_PRIVATE	</Flags>
			<Editable>      False                     </Editable>
        </clanID>
		
		<clanName>
			<Type>			STRING			</Type>
			<Flags>			CELL_PRIVATE	</Flags>
			<Editable>      False                     </Editable>
        </clanName>
		
        <trapped>
			<Type> ARRAY <of> FIXED_DICT
                    <Properties>
                        <mail>
                            <Type> MAILBOX </Type>
                        </mail>
                        <clanID>
                            <Type> INT64 </Type>
                        </clanID>
	                    <count>
		                    <Type> INT8 </Type>
	                    </count>
                    </Properties>
                </of>
            </Type>
			<Flags>			CELL_PRIVATE	</Flags>
        </trapped>

		<radius> 			<!--  TEMP -->
			<Type>			FLOAT 			</Type>
			<Flags>			CELL_PRIVATE			</Flags>
			<Default>		5.000000 			</Default>
			<Editable>		true			</Editable>
			<Widget>		RADIUS						<!--  Draw sphere in world editor  --->
				<colour>	255.000000 0.000000 0.000000 255.000000	</colour>
				<gizmoRadius>	2				</gizmoRadius>
			</Widget>
		</radius>

		<safe_zone>
			<Type>          MAILBOX         </Type>
			<Flags>			CELL_PRIVATE	</Flags>
        </safe_zone>
		
        <npcs>
			<Type> ARRAY <of> MAILBOX
                </of>
            </Type>
			<Flags>			CELL_PRIVATE	</Flags>
        </npcs>
		
		<guardians>
			<Type> ARRAY <of> MAILBOX
                </of>
            </Type>
			<Flags>			CELL_PRIVATE	</Flags>
        </guardians>
		
		 <flags>
			<Type> ARRAY <of> FIXED_DICT
                    <Properties>
                        <mail>
                            <Type> MAILBOX </Type>
                        </mail>
                        <id>
                            <Type>  ENTITY_ID  </Type>
                        </id>
						<permission>
							<Type>  ARRAY <of> ENTITY_ID </of>  </Type>
						</permission>
                        <state>
                            <Type>  INT8  </Type>
                        </state>
                        <clanID>
                            <Type>  INT64  </Type>
                        </clanID>
                        <winning_clanID>
                            <Type>  INT64  </Type>
                        </winning_clanID>
                        <level>
                            <Type>  INT8  </Type>
                        </level>
                        <progress>
                            <Type>  INT8  </Type>
                        </progress>
                    </Properties>
                </of>
            </Type>
			<Flags>			CELL_PRIVATE	</Flags>
        </flags>
    </Properties>

    <ClientMethods>
    </ClientMethods>

    <CellMethods>
		<setClanId>
			<Arg>			INT64                    </Arg>
			<Arg>			STRING                    </Arg>
		</setClanId>
		
		<spawnGuards/>
		
		 <registerNPC>
            <Arg>   MAILBOX     </Arg>
            <Arg>   INT64       </Arg>
        </registerNPC>
		
		<registersNPC>
            <Arg>   MAILBOX     </Arg>
            <Arg>   INT64       </Arg>
        </registersNPC>
		
		<registerFlag>
            <Arg>           MAILBOX             </Arg>
            <Arg>           INT8                </Arg>
            <Arg>           INT64                </Arg>
	        <Arg>           INT8                </Arg>
        </registerFlag>
		
		<registerSafeZone>
            <Arg>   MAILBOX </Arg>
			<Arg>   STRING  </Arg>
	    </registerSafeZone>
		
		<onAvatarEnter>
			<Arg> MAILBOX </Arg>
			<Arg> INT64 </Arg>
		</onAvatarEnter>
    </CellMethods>

    <BaseMethods>
		<setClanId>
			<Arg>			INT64                    </Arg>
			<Arg>			STRING                    </Arg>
		</setClanId>
		<spawnGuards/>
    </BaseMethods>
</root>