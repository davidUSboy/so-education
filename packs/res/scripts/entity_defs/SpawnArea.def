<root>
	<Implements>
		<Interface> BaseAndCell </Interface>
		<Interface> EffectsSource </Interface>
        <Interface> Teleporter </Interface>
		<Interface> TimerManagerCell </Interface>
	</Implements>
	
	<Properties>
		<radius>
			<Type>			FLOAT 			</Type>
			<Flags>			CELL_PUBLIC		</Flags>
			<Default>		20.000000			</Default>
			<Persistent>	true			</Persistent>
			<Editable>		true			</Editable>
			<Widget>		RADIUS						<!--  Draw sphere in world editor  -->
			<colour>	255.000000 0.000000 0.000000 255.000000	</colour>
			<gizmoRadius>	2	</gizmoRadius>
			</Widget>
		</radius>
		
		<safeRadius>
			<Type>			FLOAT 			</Type>
			<Flags>			CELL_PUBLIC		</Flags>
			<Default>		0.000000			</Default>
			<Persistent>	true			</Persistent>
			<Editable>		true			</Editable>
			<Widget>		RADIUS						<!--  Draw sphere in world editor  -->
			<colour>	255.000000 255.000000 0.000000 255.000000	</colour>
			<gizmoRadius>	2	</gizmoRadius>
			</Widget>
		</safeRadius>
		
		<team>
			<Type>			INT32 			</Type>
			<Flags>			CELL_PUBLIC		</Flags>
			<Editable>		true			</Editable>
			<Default> 0 </Default> <!-- non-zero value means being linked to specific team in battleground -->
		</team>
		
		<trapped>
			<Type> ARRAY <of> FIXED_DICT
					<Properties>
						<mail>
							<Type> MAILBOX  </Type>
						</mail>
						<count>
							<Type> INT8     </Type>
						</count>
						<wait>
							<Type> INT8     </Type>
						</wait>
					</Properties>
				</of>
			</Type>
			<Flags>			CELL_PUBLIC		</Flags>
			<Persistent>	false			</Persistent>
			<Editable>		false			</Editable>
		</trapped>

		<recentlyLeft>
			<Type> ARRAY <of> FIXED_DICT
					<Properties>
						<id>
							<Type> ENTITY_ID  </Type>
						</id>
						<time>
							<Type> FLOAT64     </Type>
						</time>
					</Properties>
				</of>
			</Type>
			<Flags>			CELL_PRIVATE	</Flags>
			<Persistent>	false			</Persistent>
			<Editable>		false			</Editable>
		</recentlyLeft>

		<type>
			<Type>			INT8	</Type>
			<Flags>			CELL_PUBLIC		</Flags>
			<Persistent>	false			</Persistent>
			<Editable>		true			</Editable>
			<Widget>	ENUM
			<enum>
				<value>	0	</value>
				<display>	SAFE_AND_RESPAWN	</display>
			</enum>
			<enum>
				<value>	1	</value>
				<display>	SAFE_ONLY	</display>
			</enum>
			<enum>
				<value>	2	</value>
				<display>	RESPAWN_ONLY	</display>
			</enum>
			</Widget>
		</type>
		
		<name>
			<Type>			STRING				</Type>
			<Flags>			CELL_PRIVATE		</Flags>
			<Editable>		true				</Editable>
		</name>
		
		<safeTimer>
			<Type>			INT32				</Type>
			<Flags>			CELL_PRIVATE		</Flags>
			<Editable>		false				</Editable>
		</safeTimer>
		<trapID>
			<Type>			INT32				</Type>
			<Flags>			CELL_PRIVATE		</Flags>
			<Editable>		false				</Editable>
		</trapID>
		
		<owner>
			<Type>			MAILBOX				</Type>
			<Flags>			CELL_PRIVATE		</Flags>
			<Editable>		false				</Editable>
		</owner>
		
		<clanID>
			<Type>			INT64				</Type>
			<Flags>			CELL_PRIVATE		</Flags>
			<Editable>		false				</Editable>
		</clanID>
		
		<weight>
			<Type>			INT8				</Type>
			<Flags>			CELL_PRIVATE		</Flags>
			<Default>		1					</Default>
			<Editable>		true				</Editable>
		</weight>
		
		<max_uses>
			<Type>			INT8				</Type>
			<Flags>			CELL_PRIVATE		</Flags>
			<Default>		5					</Default>
			<Editable>		true				</Editable>
		</max_uses>

        <clanIDExceptions>
			<Type>			ARRAY <of> INT64 </of>   </Type>
			<Flags>			CELL_PRIVATE		     </Flags>
        </clanIDExceptions>

		<UDOExceptionsCount>
			<Type>			UINT16			</Type>
			<Flags>			CELL_PRIVATE	</Flags>
			<Default>		0 				</Default>
		</UDOExceptionsCount>

		<volumes>
			<Type>			ARRAY <of> UDO_REF </of> </Type>
			<Flags>			CELL_PRIVATE		</Flags>
			<Persistent>	true				</Persistent>
			<Editable>		true				</Editable>
		</volumes>

		<!-- <teamID>
			<Type>			INT8   </Type>
			<Flags>			CELL_PRIVATE		</Flags>
			<Editable>		true			</Editable>
			<Widget>	ENUM
				<enum>
					<value>	0	</value>
					<display>	TEAM_NO	</display>
				</enum>
				<enum>
					<value>	1	</value>
					<display>	TEAM_RED	</display>
				</enum>
				<enum>
					<value>	2	</value>
					<display>	TEAM_BLUE	</display>
				</enum>
			</Widget> 

		</teamID>-->


	</Properties>


	<ClientMethods>
	</ClientMethods>

	<CellMethods>
		<setClanID>
			<Arg>		INT64		</Arg>
		</setClanID>
		
		<despawn/>
		
	    <updateEnemyClans>
		    <Arg> ARRAY <of> INT64 </of> </Arg>
		    <Arg>   INT64 </Arg>
	    </updateEnemyClans>

		<setType>
				<Arg>		INT8		</Arg>
		</setType>
		
		<onEnterArea>
			<Arg>		MAILBOX	</Arg>
		</onEnterArea>

		<onLeaveArea>
			<Arg>		MAILBOX	</Arg>
		</onLeaveArea>

        <onLeaveAreaID>
            <Arg>		ENTITY_ID	</Arg>
        </onLeaveAreaID>

		<onAvatarRentryArea>
			<Arg>		MAILBOX	</Arg>
		</onAvatarRentryArea>

		<setWait>
			<Arg>		ENTITY_ID	</Arg>
			<Arg>		INT8	    </Arg>
		</setWait>
	</CellMethods>

	<BaseMethods>
	</BaseMethods>



</root>



