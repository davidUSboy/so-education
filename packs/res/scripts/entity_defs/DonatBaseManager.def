<Root>
	<Implements>
		<Interface> TimerManagerBase </Interface>
	</Implements>
	
	<Properties>
		<name>
			<Type>			STRING			</Type>
			<Flags>			BASE			</Flags>
			<Default>		DonatBaseManager	</Default>
			<Identifier>	true			</Identifier>
			<Persistent>	true			</Persistent>
		</name>
		
		<bases> 
			<Type>ARRAY <of> DONATE_BASE_ENTRY </of></Type>
			<Flags>			BASE			</Flags>
			<Persistent>	true			</Persistent>
		</bases>
		
		<baseEntities>
			<Type> ARRAY <of> FIXED_DICT  
					<Properties>
						<baseName>
							<Type> STRING </Type>
						</baseName>
						
						<base>
							<Type> MAILBOX </Type>
						</base>
					</Properties>
				</of> </Type>
			<Flags>			BASE			</Flags>
			<Persistent>	false			</Persistent>
		</baseEntities>

		<baseTraders>
			<Type>	ARRAY <of> FIXED_DICT  
					<Properties>
						<baseName>
							<Type> STRING </Type>
						</baseName>

                        <npcType>
							<Type> sNPC_TYPE </Type>
						</npcType>
						
						<npc>
							<Type> MAILBOX </Type>
						</npc>
					</Properties>
				</of> </Type>
			<Flags>			BASE			</Flags>
			<Persistent>	false			</Persistent>
		</baseTraders>
		
		<deposites>
			<Type>	ARRAY <of> FIXED_DICT  
					<Properties>
						<clanID>
							<Type> INT64 </Type>
						</clanID>
						
						<deposit>
							<Type> INT64 </Type>
						</deposit>
					</Properties>
				</of> </Type>
			<Flags>			BASE			</Flags>
			<Persistent>	true			</Persistent>
		</deposites>		
	</Properties>

	<BaseMethods>
		<cleanup/>
		
		<registerBase>
			<Arg>			MAILBOX			</Arg>
			<Arg>			STRING			</Arg>
			<Arg>			STRING			</Arg>
			<Arg>			STRING			</Arg>
			<Arg> INT32 </Arg>
			<Arg> INT32 </Arg>
			<Arg> INT64 </Arg>
			<Arg> INT64 </Arg>
			<Arg> INT64 </Arg>
			<Arg> INT8 </Arg>
		</registerBase>
		
		<getSailListByLocation>
			<Arg> MAILBOX </Arg>
			<Arg> STRING </Arg>
		</getSailListByLocation>
		
		<getDonateBaseByName>
			<Arg> MAILBOX </Arg>
			<Arg> STRING </Arg>
		</getDonateBaseByName>
		
		<addPrice>
			<Arg> MAILBOX </Arg>
			<Arg> STRING </Arg>
			<Arg> STRING </Arg>
			<Arg> INT64 </Arg>
			<Arg> INT32 </Arg>
		</addPrice>

		<cashback>
			<Arg> MAILBOX </Arg>
			<Arg> INT64 </Arg>
		</cashback>
		
		<pay>
			<Arg> INT64 </Arg>			
			<Arg> INT64 </Arg>
		</pay>
		
		<getDeposit>
			<Arg> MAILBOX </Arg>
			<Arg> INT64 </Arg>
		</getDeposit>
		
		<depositBack>
			<Arg> MAILBOX </Arg>
			<Arg> INT64 </Arg>
		</depositBack>		
		
		<registerBaseNPC>
			<Arg> STRING </Arg>
			<Arg> MAILBOX </Arg>
			<Arg> sNPC_TYPE </Arg>
		</registerBaseNPC>
		
		<check_clan_stake>
			<Arg> MAILBOX </Arg>
			<Arg> INT64 </Arg>
			<Arg> INT64 </Arg>
		</check_clan_stake>

        <remove_deposit>
            <Arg> STRING </Arg>
			<Arg> INT64 </Arg>
        </remove_deposit>
		
		<get_time>
            <Arg> MAILBOX </Arg>
			<Arg> INT64 </Arg>
        </get_time>
		
		<guards_activate>
			<Arg> INT64 </Arg>
        </guards_activate>
		
		<spawnGuards>
			<Arg> STRING </Arg>
        </spawnGuards>
		
		<!-- test functions -->
		<setNewOwner>
			<Arg> STRING </Arg>
			<Arg> STRING </Arg>
			<Arg> INT64 </Arg>
		</setNewOwner>
		
		<checkBases/>
		<printBases/>
		<reset/>
		<createBaseSpace/>
	</BaseMethods>
	
	<!-- No cell methods -->
	<CellMethods/>

	<!-- No client methods -->
	<ClientMethods/>
</Root>
