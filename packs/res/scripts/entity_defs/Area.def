<root>
	<Implements>
		<Interface> BaseAndCell </Interface>
		<Interface> MappedObject </Interface>
		<Interface> TimerManagerCell </Interface>
		<Interface> TimerManagerBase </Interface>
	</Implements>

	<Properties>
		<name>			
			<Type>			STRING			</Type>
			<Flags>			CELL_PRIVATE			</Flags>
			<Persistent>	true			</Persistent>
			<Editable>		true			</Editable>
		</name>
		
		<radius> 			<!--  TEMP -->
			<Type>			FLOAT 			</Type>
			<Flags>			CELL_PRIVATE			</Flags>
			<Default>		50.000000 			</Default>
			<Persistent>	true			</Persistent>
			<Editable>		true			</Editable>
			<Widget>		RADIUS						<!--  Draw sphere in world editor  -->
				<colour>	255.000000 0.000000 0.000000 255.000000	</colour>
				<gizmoRadius>	2				</gizmoRadius>
			</Widget>
		</radius>
		
		<volumes>
			<Type>			ARRAY <of> UDO_REF </of> </Type>
			<Flags>			CELL_PRIVATE		</Flags>
			<Persistent>	true				</Persistent>
			<Editable>		true				</Editable>
		</volumes>

		<anomalyPoints>
			<Type>			ARRAY <of> UDO_REF </of> </Type>
			<Flags>			CELL_PRIVATE		</Flags>
			<Persistent>	true				</Persistent>
			<Editable>		true				</Editable>
		</anomalyPoints>

		<occupiedAnomalyPoints>
			<Type>			ARRAY <of> INT32 </of> </Type>
			<Flags>			CELL_PRIVATE		</Flags>
			<Persistent>	true				</Persistent>
		</occupiedAnomalyPoints>
		
		<!-- Used while loading UDOs to count loading time -->
		<UDOExceptionsCount>			
			<Type>			UINT16			</Type>
			<Flags>			CELL_PRIVATE	</Flags>
			<Default>		0 				</Default>
		</UDOExceptionsCount>		

<!-- _____________________________________________________________________ -->

		<anomalyMin>
			<Type>			INT32				</Type>
			<Flags>			CELL_PRIVATE		</Flags>
			<Persistent>	true				</Persistent>
			<Editable>		true				</Editable>
		</anomalyMin>

		<anomalyMax>
			<Type>			INT32				</Type>
			<Flags>			CELL_PRIVATE		</Flags>
			<Persistent>	true				</Persistent>
			<Editable>		true				</Editable>
		</anomalyMax>

		<terrainType>
			<Type>			TERRAIN_TYPE		</Type>
			<Flags>			CELL_PRIVATE		</Flags>
			<Persistent>	true			</Persistent>
			<Editable>		true			</Editable>
		</terrainType>
		
		<difficulty>
			<Type>			INT32			</Type>
			<Flags>			CELL_PRIVATE			</Flags>
			<Default>		10 				</Default>
			<Persistent>	true			</Persistent>
			<Editable>		true			</Editable>
		</difficulty>

		<trapped>
			<Type>			ARRAY <of>ENTITY_ID</of>	</Type>
			<Flags>			CELL_PRIVATE	</Flags>
			<Persistent>	false			</Persistent>
			<Editable>		false			</Editable>
		</trapped>

		<anomalies>
			<Type>			ARRAY <of>ENTITY_ID</of>	</Type>
			<Flags>			CELL_PRIVATE	</Flags>
			<Persistent>	false			</Persistent>
			<Editable>		false			</Editable>
		</anomalies>
		
		<giveQuests>
			<Type>	ARRAY	<of>	QUESTID	</of>	</Type>
			<Flags>	CELL_PRIVATE					</Flags>
			<Editable>		true			</Editable>
		</giveQuests>
	</Properties>

	<CellMethods>
		<onEnterArea>
			<Arg>		ENTITY_ID	</Arg>
		</onEnterArea>

		<onLeaveArea>
			<Arg>		ENTITY_ID	</Arg>
		</onLeaveArea>
		
		<onAnomalyDespawned>
			<Arg>		ENTITY_ID	</Arg>
			<Arg>		INT32	</Arg> <!-- point number -->
		</onAnomalyDespawned>
		
		<onAnomalyInitialized>
			<Arg>		MAILBOX				</Arg>	<!-- Anomaly Cell Mailbox --->
		</onAnomalyInitialized>
	</CellMethods>

	<BaseMethods>
		<register/>
	</BaseMethods>
	
</root>

