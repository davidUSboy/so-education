<Root>
	<Implements>
		<Interface> TimerManagerBase </Interface>
	</Implements>
	<Properties>
		<!--	Globally registered name. -->
		<name>
			<Type>			STRING			</Type>
			<Flags>			BASE			</Flags>
			<Default>		Shop			</Default>
			<Identifier>	true			</Identifier>
			<Persistent>	true			</Persistent>
		</name>

		
		<!-- dict {"avatar name": index in array_catalog} -->
		<catalog_dict_names_id> 
			<Type>			PYTHON			</Type>
			<Flags>			BASE			</Flags>		
		</catalog_dict_names_id>
		
		

		<!--  [ (index, sunindexItem), ]  -->

		<!-- list indexs array_catalog sorted by cost-->
		<catalog_id_by_cost> 
			<Type>			ARRAY <of> ARRAY <of> INT32 </of> </of>			</Type>
			<Flags>			BASE			</Flags>		
		</catalog_id_by_cost>

		
		<!-- list indexs array_catalog sorted by username-->
		<catalog_id_by_username> 
			<Type>			ARRAY <of> ARRAY <of> INT32 </of> </of>			</Type>
			<Flags>			BASE			</Flags>		
		</catalog_id_by_username>	

		
		<!-- list indexs array_catalog sorted by time lost-->
		<catalog_id_by_hoursLeft> 
			<Type>			ARRAY <of> ARRAY <of> INT32 </of> </of>			</Type>
			<Flags>			BASE			</Flags>		
		</catalog_id_by_hoursLeft>
			
		<!-- list indexs array_catalog sorted by itemname RUS -->
		<catalog_id_by_itemname> 
			<Type>			ARRAY <of> ARRAY <of> INT32 </of> </of>			</Type>
			<Flags>			BASE			</Flags>		
		</catalog_id_by_itemname>
	
	
		
		


		<!-- list  data in BD -->
 		<catalog>
			<Type>			ARRAY <of> USI </of>		</Type>
			<!-- <Type>			ARRAY <of> INT32 </of>		</Type> -->
			<Flags>			BASE			</Flags>		
			<Persistent>	true			</Persistent>
		</catalog> 
		
		
	
	
	
	
	
	
	
	
	
	</Properties>

	<BaseMethods>
	
		<cleanup/>
		
		<testPrint/>
		
		<recreateSortedCatalogs/>
		
		<removeAvatarCatalogInShop>
			<Arg>	STRING			</Arg>	<!-- avatarName -->
		</removeAvatarCatalogInShop>
		
		
		
		
		
		<removeItemsInShop>
			<Arg>	STRING			</Arg>	<!-- avatarName -->
			<Arg>	STRING			</Arg>	<!-- BankName -->
			<Arg>	ITEM_ID			</Arg>	<!-- complexItemID -->
		</removeItemsInShop>	
		
		
		
		<getSellerItemsInShop>
			<Arg>	STRING			</Arg>	<!-- avatarName -->
			<Arg>	MAILBOX			</Arg>	<!-- seller Mailbox -->
		</getSellerItemsInShop>	

		
		<itemToShop>
			<Arg>	STRING			</Arg>	<!-- avatarName -->
			<Arg>	COMPLEX_ITEM	</Arg>	<!-- item -->
			<Arg>	INT64			</Arg>	<!-- price -->
			<Arg>	INT64			</Arg>	<!-- price_tax -->
			<Arg>	STRING			</Arg>	<!-- BankName -->
			<Arg>	INT32			</Arg>	<!-- hoursLeft -->
			<Arg>	MAILBOX			</Arg>	<!-- seller Base Mailbox -->
		</itemToShop>
		
		
		<buyItemFromShop>
			<Arg>	STRING			</Arg>	<!-- sellerAvatarName -->
			<Arg>	ITEM_ID			</Arg>	<!-- complexItemID -->
			<Arg>	STRING			</Arg>	<!-- buyerName -->
			<Arg>	INT64			</Arg>	<!-- price -->
			<Arg>	STRING			</Arg>	<!-- BankName -->			
			<Arg>	MAILBOX			</Arg>	<!-- buyerBMailbox -->
		</buyItemFromShop>		
		
		<cancelSale>
			<Arg>	STRING			</Arg>	<!-- sellerAvatarName -->
			<Arg>	ITEM_ID			</Arg>	<!-- complexItemID -->
			<Arg>	STRING			</Arg>	<!-- BankName -->		
			<Arg>	MAILBOX			</Arg>	<!-- buyerBMailbox -->
		</cancelSale>
		
		
		<getShopCatalogFiltered>
			<Arg>		INT8				</Arg> <!-- sortFlag -->
			<Arg>		INT8				</Arg> <!-- forward -->
			<Arg>		INT32				</Arg> <!-- page_start -->
			<Arg>		INT32				</Arg> <!-- page_len -->		
			<Arg>	ARRAY <of>INT32</of>	</Arg> <!-- tags -->
			<Arg>		MAILBOX				</Arg> <!-- client mailbox -->

		</getShopCatalogFiltered>	

		
		<getCatalogAll>		
			<Arg>	ARRAY <of>INT32</of>	</Arg> <!-- tags -->
			<Arg>		MAILBOX				</Arg> <!-- client mailbox -->
		</getCatalogAll>
		
		
		
		
	</BaseMethods>
	
	<!-- No cell methods -->
	<CellMethods/>

	<!-- No client methods -->
	<ClientMethods/>
</Root>
